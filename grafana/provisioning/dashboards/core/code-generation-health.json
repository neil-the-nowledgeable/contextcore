            "showPoints": "auto",
            "stacking": {
              "group": "A",
              "mode": "none"
            }
          },
          "unit": "none"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "estimated" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "actual" },
            "properties": [
              { "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "calcs": ["mean", "max"],
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "refId": "A",
          "queryType": "traceqlSearch",
          "query": "{ name = \"code_generation.preflight\" && resource.project.id =~ \"${project}\" } | select(span.gen_ai.code.estimated_lines)"
        },
        {
          "datasource": { "type": "tempo", "uid": "tempo" },
          "refId": "B",
          "queryType": "traceqlSearch",
          "query": "{ name = \"code_generation.verify\" && resource.project.id =~ \"${project}\" } | select(span.gen_ai.code.actual_lines)"
