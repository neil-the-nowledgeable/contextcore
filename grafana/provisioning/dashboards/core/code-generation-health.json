    "contextcore",
    "code-generation",
    "truncation-prevention",
    "prime-contractor"
  ],
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "schemaVersion": 39,
  "refresh": "30s",
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "version": 1,
  "weekStart": "",
  "templating": {
    "list": [
      {
        "name": "project",
        "label": "Project",
        "type": "custom",
        "current": {
          "text": "contextcore",
          "value": "contextcore"
        },
        "options": [
          {
            "text": "contextcore",
            "value": "contextcore",
            "selected": true
          },
          {
            "text": "all",
            "value": ".*"
          }
        ],
        "query": "contextcore,all"
      }
    ]
